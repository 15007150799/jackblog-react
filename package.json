{
  "name": "jackblog-react",
  "version": "2.1.4",
  "description": "Jackblog react ç‰ˆ",
  "repository": "https://github.com/jackhutu/jackblog-react",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --sequential build start-prod",
    "dev": "npm-run-all --sequential build-server:dev start-dev",
    "build": "npm-run-all clean --parallel build-all:prod",
    "build-server:dev": "better-npm-run build-dev-server",
    "build-all:prod": "better-npm-run build-prod",
    "start-dev": "better-npm-run nodemon-dev",
    "start-prod": "better-npm-run node-prod",
    "clean": "rimraf dist",
    "pm2-start": "pm2 start --no-daemon process.json"
  },
  "betterScripts": {
    "build-dev-server": {
      "command": "webpack --colors --config ./webpack/webpack.config.dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "nodemon-dev": {
      "command": "nodemon server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "node-prod": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-prod": {
      "command": "webpack --colors --config ./webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/jackhutu/jackblog-react/issues"
  },
  "homepage": "https://github.com/jackhutu/jackblog-react",
  "dependencies": {
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.11",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.3",
    "eslint": "^3.7.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "immutable": "^3.8.1",
    "jackblog-sass": "^1.0.6",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^1.0.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-cookie": "^0.4.8",
